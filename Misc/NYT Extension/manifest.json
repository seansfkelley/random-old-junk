{
   "manifest_version": 2,
   "background": { "scripts": [ "background.js" ] },
   "description": "Get rid of annoying NYT javascript.",
   "icons": {
      "128": "icon128.png"
   },
   "name": "NYT Extension",
   "page_action": {
      "default_icon": "icon128.png"
   },
   "permissions": [ "tabs", "http://*.nytimes.com/*" ],
   "version": "1.0"
}
